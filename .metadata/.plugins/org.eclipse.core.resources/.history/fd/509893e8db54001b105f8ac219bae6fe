package br.senai.sp.jandira;

import br.senai.sp.jandira.model.Conta;

public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Conta contaRenanPrazeres;
		Conta contaPedro;
		
		contaRenanPrazeres = new Conta();
		contaPedro = new Conta();
		
		//**CONTA RENAN PRAZERES
		
		contaRenanPrazeres.numero = "111-0";
		contaRenanPrazeres.titular = "RenanPrazeres";
		contaRenanPrazeres.tipo = "corrente";
		contaRenanPrazeres.saldo = 2000.0;
		contaRenanPrazeres.ativa = true;
		
		//*** conta Pedro
		contaPedro.numero = "222-0";
		contaPedro.titular = "Pedro Cabral";
		contaPedro.tipo = "Poupança";
		contaPedro.saldo = 1500.0;
		contaPedro.ativa = true;
		
		
		System.out.println("nome: " + contaPedro.titular);
		System.out.println("saldo: " + contaPedro.saldo);
		
		System.out.println("-------------------------------------------------------");
		
	System.out.println("nome: " + contaRenanPrazeres.titular);
	System.out.println("Saldo: R$ " + contaRenanPrazeres.saldo);
		
	}

}
